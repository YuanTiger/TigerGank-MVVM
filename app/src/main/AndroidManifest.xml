<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.my.gank">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".base.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:usesCleartextTraffic="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.TestActivity" />
        <activity android:name=".test.lifecycle.MyLocationTestActivity" />
        <activity android:name=".test.viewmodel.one.ViewModelTestActivity" />
        <activity android:name=".test.viewmodel.two.ViewModelTestActivity_2" />
        <activity android:name=".test.databinding.one.DataBindingTestActivity" />
        <activity android:name=".test.databinding.two.DataBindingTestActivity_2" />
        <activity android:name=".test.databinding.three.DataBindingTestActivity_3" />
        <activity android:name=".test.databinding.four.DataBindingTestActivity_4" />
        <activity android:name=".test.databinding.five.DataBindingTestActivity_5" />
        <activity android:name=".test.databinding.six.DataBindingTestActivity_6" />

        <service android:name=".test.lifecycle.MyLocationService"/>
    </application>

</manifest>